{
  "backbone": {
    "name": "resnet50",
    "weights": null,
    "grad": true
  },
  "batch_size": 256,
  "column": "CATAMI Substrate",
  "num_workers": 12,
  "max_epochs": 400,
  "dims": [],
  "dropout": 0.7,
  "backbone_params": {
    "zero_init_residual": true
  },
  "optimizer": {
    "name": "adamw",
    "lr": 3e-05,
    "weight_decay": 1e-05,
    "extra_optimizer_args": {
      "betas": [
        0.9,
        0.999
      ]
    }
  },
  "scheduler": {
    "name": "warmup_cosine",
    "interval": "epoch",
    "warmup_epochs": 10,
    "warmup_start_lr": 3e-06,
    "min_lr": 3e-06
  }
}
